bind-addr 127.0.0.1
bind-port 5300

china-dns tcp://::1#5306
trust-dns tcp://::1#5305

chnlist-file /var/lib/lists/chnlist.txt
chnlist-file ${ROUTER_DATA_PATH}/dns/dispatch/force.china.list
chnlist-file ${ROUTER_DATA_PATH}/dns/dispatch/force.china.list


gfwlist-file /var/lib/lists/gfwlist.txt
gfwlist-file ${ROUTER_DATA_PATH}/dns/dispatch/force.oversea.list

group blacklist
group-dnl ${ROUTER_DATA_PATH}/dns/dispatch/blacklist.list
group-upstream tcp://::1#8807

add-tagchn-ip
ipset-name4 inet@router@chnip4
ipset-name6 inet@router@chnip6
add-taggfw-ip inet@router@gfwip4,inet@router@gfwip6

timeout-sec 10

cache 0

verdict-cache 60000
verdict-cache-db ${ROUTER_DATA_PATH}/dns/dispatch/verdict-cache-db

hosts ${__dirname}/hosts
hosts ${ROUTER_DATA_PATH}/dns/dispatch/hosts

# verbose

config ${ROUTER_DATA_PATH}/dns/dispatch/config
