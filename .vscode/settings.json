{
	"search.exclude": {
		"**/.vscode/local-history": true,
		".config/**": true,
		".outputs/**": true,
	},
	"files.associations": {
		"*.json": "jsonc"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"python.analysis.exclude": [
		"**/.*",
		".config"
	],
	"remote.autoForwardPorts": false,
	"python.envFile": "${workspaceFolder}/.env"
}
